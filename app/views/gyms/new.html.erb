<h2>Add something you think is useless to workout with.</h2>

<%= form_tag '/gyms', method: :post do %>
  <div>
    <%= label_tag :equipment_type %>
    <%= text_field_tag :equipment_type %>
  </div>
  <div>
    <%= label_tag :description %>
    <%= text_field_tag :description %>
  </div>
  <div>
    <%= label_tag :good_for %>
    <%= text_field_tag :good_for %>
  </div>
  <div>
    <%= submit_tag "Add Equipment" %>
  </div>

  <% end %>